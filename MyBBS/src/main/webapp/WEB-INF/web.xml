<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login_process</servlet-name>
        <servlet-class>servlet.login_process</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newtopic_process</servlet-name>
        <servlet-class>servlet.newtopic_process</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateUser_process</servlet-name>
        <servlet-class>servlet.updateUser_process</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register_process</servlet-name>
        <servlet-class>servlet.register_process</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newReply_process</servlet-name>
        <servlet-class>servlet.newReply_process</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteTopic_process</servlet-name>
        <servlet-class>servlet.deleteTopic_process</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sendMessage_process</servlet-name>
        <servlet-class>servlet.sendMessage_process</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateUserGroup</servlet-name>
        <servlet-class>servlet.updateUserGroup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BanUser</servlet-name>
        <servlet-class>servlet.BanUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnbanUser</servlet-name>
        <servlet-class>servlet.UnbanUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteReply_process</servlet-name>
        <servlet-class>servlet.deleteReply_process</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login_process</servlet-name>
        <url-pattern>/login_process</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newtopic_process</servlet-name>
        <url-pattern>/newtopic_process</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateUser_process</servlet-name>
        <url-pattern>/updateUser_process</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register_process</servlet-name>
        <url-pattern>/register_process</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newReply_process</servlet-name>
        <url-pattern>/newReply_process</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteTopic_process</servlet-name>
        <url-pattern>/deleteTopic_process</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sendMessage_process</servlet-name>
        <url-pattern>/sendMessage_process</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateUserGroup</servlet-name>
        <url-pattern>/updateUserGroup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BanUser</servlet-name>
        <url-pattern>/BanUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnbanUser</servlet-name>
        <url-pattern>/UnbanUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteReply_process</servlet-name>
        <url-pattern>/deleteReply_process</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
